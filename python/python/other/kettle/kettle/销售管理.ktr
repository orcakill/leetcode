    <sql>select a.rq,
       '合计' dwmc,
       a.rql sq,
       a.rjql sqrjhsp,
       b.rql gq,
       b.rql,
       b.rjql gqrjhsp
  from (select rq, rql, rjql
          from epbp_yqxs_trqxsrb
         where bbfl = '收气'
           and xmmc = '合计'
        union all
        select rq, rql, rjql
          from epbp_yqxs_trqxsrb
         where bbfl = '收气'
           and xmmc = '收气合计') a,
       (select rq, rql, rjql
          from epbp_yqxs_trqxsrb
         where bbfl = '外供'
           and xmmc = '合计'
        union all
        select rq, rql, rjql
          from epbp_yqxs_trqxsrb
         where bbfl = '外供'
           and xmmc = '外供合计') b
 where a.rq = b.rq
   and a.rq >= trunc(sysdate, 'dd') - 10
 order by rq desc
</sql>
